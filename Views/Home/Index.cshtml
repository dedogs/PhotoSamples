@model IEnumerable<string>

@{
    ViewBag.Title = "Home Page";
}

<section>
    <div class="a-spinner-wrapper"><span class="a-spinner a-spinner-medium"></span>Please Wait...</div>
</section>

<script>

    function preLoadedImages() {
        var src = [];
        @{
            string[] src = Model.ToArray();

            for (int i = 0; i < Model.Count(); i++) {
                @:src[@i] = '@Html.Raw(src[i])';
                                     }
           }

        return src;
    }
</script>